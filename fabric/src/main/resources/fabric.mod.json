{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "Greenhouse Team"
    ],
    "contributors": [
        "MerchantPug"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/enchantment-config/",
        "sources": "https://github.com/GreenhouseTeam/enchantment-config/",
        "issues": "https://github.com/GreenhouseTeam/enchantment-config/issues"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "dev.greenhouseteam.enchantmentconfig.impl.EnchantmentConfigFabric"
        ],
        "enchantmentconfig": [
            "dev.greenhouseteam.enchantmentconfig.impl.vanilla.VanillaEnchantmentConfigEntry"
        ]
    },
    "mixins": [
        "enchantmentconfig.mixins.json",
        "enchantmentconfig.fabric.mixins.json"
    ],
    "accessWidener" : "${mod_id}.accesswidener",
    "depends": {
        "fabricloader": ">=0.15",
        "fabric": "*",
        "minecraft": "~1.20.5",
        "java": ">=21"
    }
}